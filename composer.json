{
  "name": "algolia/algoliasearch-magento-2",
  "description": "Algolia Search & Discovery extension for Magento 2",
  "type": "magento2-module",
  "license": ["MIT"],
  "version": "3.14.0",
  "require": {
    "php": "~8.1|~8.2|~8.3",
    "magento/framework": "~103.0",
    "algolia/algoliasearch-client-php": "^4.0@beta",
    "guzzlehttp/guzzle": "^6.3.3|^7.3.0",
    "ext-json": "*",
    "ext-PDO": "*",
    "ext-mbstring": "*",
    "ext-dom": "*"
  },
  "suggest": {
      "algolia/algoliasearch-inventory-magento-2": "Algolia Search Inventory module for Magento 2.3.x,2.4.x and Algolia Search 1.12+ extension"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require-dev": {
    "phpcompatibility/php-compatibility": "^9.2"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "Algolia\\AlgoliaSearch\\": ""
    }
  }
}
